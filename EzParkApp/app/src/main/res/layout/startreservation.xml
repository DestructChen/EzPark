<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context=".startReservation"
    >

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="@dimen/button_height"
        android:text="@string/title_availableSpaces"
        android:textSize="35sp"
        android:id="@+id/title_availbleSpaces"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/backbutton"
        android:text="Cancel"
        android:onClick="home"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />


    <EditText
        android:id="@+id/textreceive"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Enter plate number"
        android:layout_above="@+id/button3"
        android:layout_alignRight="@+id/title_availbleSpaces"
        android:layout_alignEnd="@+id/title_availbleSpaces" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="registeruser"
        android:onClick="registeruser"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <GridView
        android:id="@+id/gridview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:numColumns="3"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="150dp"
        />


</RelativeLayout>